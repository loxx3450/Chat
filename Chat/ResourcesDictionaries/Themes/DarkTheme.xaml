<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!--Global-->
    <SolidColorBrush x:Key="MainBackground">#0d1117</SolidColorBrush>
    <SolidColorBrush x:Key="SecondaryBackground">#161b22</SolidColorBrush>
    <SolidColorBrush x:Key="MainForeground">#FFFFFF</SolidColorBrush>
    <SolidColorBrush x:Key="MainBorderBrush">#30363d</SolidColorBrush>
    <SolidColorBrush x:Key="MainButtonBackground">#f0f8ff</SolidColorBrush>
    
    <!--TitleBar-->
    <SolidColorBrush x:Key="TitleBarBackground">#11171f</SolidColorBrush>
    <SolidColorBrush x:Key="TitleBarBorderBrush">#000000</SolidColorBrush>
    <SolidColorBrush x:Key="MouseOverTitleBarButtonColor">#474747</SolidColorBrush>
    <SolidColorBrush x:Key="IsPressedTitleBarButtonColor">#965659</SolidColorBrush>
    
    <!--MainWindow-->
    <SolidColorBrush x:Key="WindowBorderBrush">#000000</SolidColorBrush>

</ResourceDictionary>